          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="channelId">
          <xs:annotation>
            <xs:documentation>Channel to get info on</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="accessTokenId">
          <xs:annotation>
            <xs:documentation>The id of the access token that will be used to authenticate the call</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/slack" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="GetChannelByNameType" substitutionGroup="mule:abstract-message-processor" name="get-channel-by-name">
    <xs:annotation>
      <xs:documentation>This processor returns information about a team channel specifyng the name.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="GetChannelByNameType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="channelName">
          <xs:annotation>
            <xs:documentation>Channel name to get info on</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="accessTokenId">
          <xs:annotation>
            <xs:documentation>The id of the access token that will be used to authenticate the call</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/slack" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="CreateChannelType" substitutionGroup="mule:abstract-message-processor" name="create-channel">
    <xs:annotation>
      <xs:documentation>This processor is used to create a channel.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="CreateChannelType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="channelName">
          <xs:annotation>
            <xs:documentation>Name of channel to create</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="accessTokenId">
          <xs:annotation>
            <xs:documentation>The id of the access token that will be used to authenticate the call</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/slack" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="RenameChannelType" substitutionGroup="mule:abstract-message-processor" name="rename-channel">
    <xs:annotation>
      <xs:documentation>This method renames a team channel. The only people who can rename a channel are team admins, or the person that originally created the channel. Others will recieve a "not_authorized" error.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="RenameChannelType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="channelId">
          <xs:annotation>
            <xs:documentation>Channel to rename</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="channelName">
          <xs:annotation>
            <xs:documentation>New channel name</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="accessTokenId">
          <xs:annotation>
            <xs:documentation>The id of the access token that will be used to authenticate the call</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/slack" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="JoinChannelType" substitutionGroup="mule:abstract-message-processor" name="join-channel">
    <xs:annotation>
      <xs:documentation>This method is used to join a channel. If the channel does not exist, it is created.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="JoinChannelType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="channelName">
          <xs:annotation>
            <xs:documentation>Name of the channel to Join</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="accessTokenId">
          <xs:annotation>
            <xs:documentation>The id of the access token that will be used to authenticate the call</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/slack" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="LeaveChannelType" substitutionGroup="mule:abstract-message-processor" name="leave-channel">
    <xs:annotation>
      <xs:documentation>This processor is used to leave a channel.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="LeaveChannelType">
